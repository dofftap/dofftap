{
  "name": "dofftap",
  "version": "1.0.0",
  "description": "dofftap.com",
  "scripts": {
    "sass": "sass --watch --update --style=expanded scss:css",
    "sass-dev": "sass --watch --update --style=expanded scss:css",
    "sass-prod": "sass --no-source-map --style=compressed scss:css",
    "browser-sync": "browser-sync start --server --startPath \"index.html\" --files \"**/*\" --directory --no-notify --no-open",
    "start": "npm-run-all --parallel sass browser-sync"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teh-grza/dofftap.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "sass": "^1.69.5"
  },
  "dependencies": {
    "browser-sync": "^3.0.2",
    "npm-run-all": "^4.1.5"
  },
  "bugs": {
    "url": "https://github.com/teh-grza/dofftap/issues"
  },
  "homepage": "https://github.com/teh-grza/dofftap#readme"
}
